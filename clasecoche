public class MyClass {
    public static void main(String args[]) {
      SumarTresNumeros(2,4,5);
      
      Coche miCoche=new Coche();
      miCoche.TotalPuertas(3);
      
      
      
    }
    public static int SumarTresNumeros(int a,int b,int c){
        
    int resultado=a+b+c;
    System.out.println(resultado);
    return resultado;
}
}
class Coche{
   
    public int TotalPuertas(int Puertas){
        int resultadoPuertas=0;
        resultadoPuertas+=Puertas+1;
        System.out.println(resultadoPuertas);
        return resultadoPuertas;
    }
}
